<odoo>
    <data noupdate="1">

        <record model="account.account" id="account_payable">
            <field name="company_id" ref="base_fiscal_company.company_fiscal_mother"/>
            <field name="code">CAE_401</field>
            <field name="name">CAE - Payable - (test) </field>
            <field name="user_type_id" ref="account.data_account_type_payable"/>
            <field name="reconcile" eval="True" />
        </record>

        <record model="account.account" id="account_receivable">
            <field name="company_id" ref="base_fiscal_company.company_fiscal_mother"/>
            <field name="code">CAE_411</field>
            <field name="name">CAE - Receivable - (test) </field>
            <field name="user_type_id" ref="account.data_account_type_receivable"/>
            <field name="reconcile" eval="True" />
        </record>

        <!-- Bank and Cash Part -->
        <record model="account.account" id="account_bank">
            <field name="company_id" ref="base_fiscal_company.company_fiscal_mother"/>
            <field name="code">CAE_512</field>
            <field name="name">CAE - Bank - (test)</field>
            <field name="user_type_id" ref="account.data_account_type_liquidity"/>
        </record>

        <record model="account.account" id="account_cash">
            <field name="company_id" ref="base_fiscal_company.company_fiscal_mother"/>
            <field name="code">CAE_581</field>
            <field name="name">CAE - Cash - (test)</field>
            <field name="user_type_id" ref="account.data_account_type_liquidity"/>
        </record>

        <!-- Expense Part-->
        <record model="account.account" id="account_expense">
            <field name="company_id" ref="base_fiscal_company.company_fiscal_mother"/>
            <field name="code">CAE_601</field>
            <field name="name">CAE - Expenses - (test)</field>
            <field name="user_type_id" ref="account.data_account_type_expenses"/>
        </record>

        <!-- Income Part-->
        <record model="account.account" id="account_sale">
            <field name="company_id" ref="base_fiscal_company.company_fiscal_mother"/>
            <field name="code">CAE_701</field>
            <field name="name">CAE - Product Sales - (test)</field>
            <field name="user_type_id" ref="account.data_account_type_revenue"/>
        </record>

    </data>
</odoo>
